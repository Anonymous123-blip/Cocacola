extends layout

block vars
	- var title = 'online'

block head_end
	link(rel="stylesheet" href="../css/index.css")
	link(rel='preconnect' href='https://fonts.gstatic.com')
	link(href='https://fonts.googleapis.com/css2?family=Montserrat:wght@100;500&display=swap' rel='stylesheet')
	link(href='https://unpkg.com/aos@2.3.1/dist/aos.css' rel='stylesheet')
	script(src='https://unpkg.com/aos@2.3.1/dist/aos.js')

block content
	nav.navbar.navbar-expand-lg.navbar-light.container-fluid.menu
		a.navbar-brand.menu-logo(href='#')
			img(src="../image/logo coke.png" alt="responsive")
		button.navbar-toggler(type='button' data-toggle='collapse' data-target='#navbarNav' aria-controls='navbarNav' aria-expanded='false' aria-label='Toggle navigation')
			span.navbar-toggler-icon
		#navbarNav.collapse.navbar-collapse
			ul.navbar-nav
				li.nav-item.active
					a.nav-link(href='#') TRANG CHỦ
				li.nav-item.active
					a.nav-link(href='#') ỨNG TUYỂN
				li.nav-item.active
					a.nav-link(href='#') 
						span(style="color: red") ASPIRE 
						| LÀ GÌ?
				li.nav-item.active
					a.nav-link(href='#') 
						| LÝ DO CHỌN 
						span(style="color: red") ASPIRE
				li.nav-item.active
					a.nav-link(href='#') QUY TRÌNH THAM GIA
				li.nav-item.active
					a.nav-link(href='#') TIÊU CHÍ TÌM KIẾM
	//-end menu ************************************************
	section.container-fluid.banner
		.row
			.col-md-12(data-aos='zoom-in')
				.banner-img
					img(src="../image/Rectangle 6@2x.png" alt="responsive")
		.row
			.col-sm-6.col-md-6(data-aos='zoom-in')
				.banner-kv.clearfix
					img.float-right(src="../image/hinh kv.png" alt="responsive")
			.col-sm-6.col-md-6(data-aos='zoom-in')
				.banner-aspire
					img(src="../image/dung bo lo.png" alt="responsive")
					.text
						h1 
							span(style="color: red; font-weight: bold") A
							span(style="font-weight: lighter") SPIRE
						h1 
							span(style="color: red; font-weight: bold") F
							|UTURE
						h1 
							span(style="color: red; font-weight: bold") L
							|EADER
					.text-coca
						p Từ 
							span(style="font-weight: bold") Coca-Cola 
							| Việt Nam
						p #1 Nhà Tuyển Dụng Yêu Thích Nhất 
						p Trở thành Nhà Lãnh đạo Thế hệ mới ngay hôm nay!
					.button-position
						button.btn.button(onclick='newFunction()')(type='submit') Ứng tuyển tại đây
		.row
			.col-md-4(data-aos='zoom-in')
				.banner-bear
					img(src="../image/gau s1.png" alt="responsive")
			.col-md-8(data-aos='zoom-in')
				.banner-form
					.row
						.col-md-6
							.banner-form-in
								.banner-form-in-user
									input(type='text' placeholder='Họ và tên')
									i.fa.fa-user(aria-hidden='true')
								.banner-form-in-mail
									input(type='text' placeholder='Email')
									i.fa.fa-envelope-o(aria-hidden='true')
								.banner-form-in-phone
									input(type='text' placeholder='Số điện thoại')
									i.fa.fa-phone(aria-hidden='true')
								.banner-form-in-score
									input(type='text' placeholder='Điểm trung bình Đại học')
									i.fa.fa-file-text-o(aria-hidden='true')
						.col-md-6
							.banner-form-in
								.banner-form-in-user
									input(type='text' placeholder='Năm tốt nghiệp đại học')
									i.fa.fa-graduation-cap(aria-hidden='true')
								.banner-form-in-mail
									input(type='text' placeholder='Địa điểm mong muốn làm việc')
									i.fa.fa-map-marker(aria-hidden='true')
								.banner-form-in-phone
									input(type='text' placeholder='Vị trí ứng tuyển')
									i.fa.fa-briefcase(aria-hidden='true')
								.banner-form-in-score
									input(type='text' placeholder='Hồ sơ của bạn')
									i.fa.fa-file-o(aria-hidden='true')
									i.fa.fa-upload(aria-hidden='true' style="left: 85%")
					.row
						.col-md-12.button-done
								button.btn.button(onclick='newFunction()')(type='submit') Hoàn thành							
	section.container-fluid.introduce
		.row
			.col-md-12(data-aos='zoom-in')
				.introduce-img
					img(src="../image/gioi thieu.png" alt="responsive")
				.text-future
					h1 
						span(style="color:red") ASPIRE 
						| FUTURE LEADERS
				.text-leader(data-aos='zoom-in')
